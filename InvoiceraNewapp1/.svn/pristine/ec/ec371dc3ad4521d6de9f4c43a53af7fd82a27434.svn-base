<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_walkthrough"
    android:scrollbars="none">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/layout_default_margin" >

        <LinearLayout
            android:id="@+id/Linear_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffff"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/contact_name_lin"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/contact_name_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:contentDescription="none"
                    android:padding="9dp"
                    android:src="@drawable/user_icon" />

                <EditText
                    android:id="@+id/contact_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:hint="Contact Name"
                    android:paddingEnd="@dimen/Error_end_padding"
                    android:inputType="textEmailAddress"
                    android:paddingLeft="13dp"
                    android:singleLine="true"
                    android:textColor="#3C3C3C"
                    android:textColorHint="@color/lightgray02" />
            </LinearLayout>

            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#B8B8B8" />

            <LinearLayout
                android:id="@+id/Company_name_lin"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/Company_name_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:contentDescription="none"
                    android:padding="9dp"
                    android:src="@drawable/pass_icon" />

                <EditText
                    android:id="@+id/Company_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:hint="Company Name"
                    android:paddingEnd="@dimen/Error_end_padding"
                    android:inputType="textEmailAddress"
                    android:paddingLeft="13dp"
                    android:singleLine="true"
                    android:textColor="#3C3C3C"
                    android:textColorHint="@color/lightgray02" />
            </LinearLayout>

            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#B8B8B8" />

            <LinearLayout
                android:id="@+id/Email"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/email_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:contentDescription="none"
                    android:padding="9dp"
                    android:src="@drawable/email" />

                <EditText
                    android:id="@+id/email_edit"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:hint="Email"
                    android:inputType="textEmailAddress"
                    android:paddingLeft="13dp"
                    android:singleLine="true"
                    android:paddingEnd="@dimen/Error_end_padding"
                    android:textColor="#3C3C3C"
                    android:textColorHint="@color/lightgray02" />
            </LinearLayout>

            <View
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#B8B8B8" />

            <LinearLayout
                android:id="@+id/User_name_lin"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/User_name_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:contentDescription="none"
                    android:padding="9dp"
                    android:src="@drawable/user_name" />

                <EditText
                    android:id="@+id/User_name_edit"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:hint="User Name"
                    android:inputType="textEmailAddress"
                    android:paddingLeft="13dp"
                    android:singleLine="true"
                    android:paddingEnd="@dimen/Error_end_padding"
                    android:textColor="#3C3C3C"
                    android:textColorHint="@color/lightgray02" />
            </LinearLayout>

            <View
                android:id="@+id/view4"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#B8B8B8" />

            <LinearLayout
                android:id="@+id/Password_lin"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/Password_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:contentDescription="none"
                    android:padding="9dp"
                    android:src="@drawable/password" />

                <EditText
                    android:id="@+id/Password_edit"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:hint="Password"
                    android:inputType="textPassword"
                    android:paddingLeft="13dp"
                    android:singleLine="true"
                    android:paddingEnd="@dimen/Error_end_padding"
                    android:textColor="#3C3C3C"
                    android:textColorHint="@color/lightgray02" />
            </LinearLayout>

            <View
                android:id="@+id/view5"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#B8B8B8" />

            <LinearLayout
                android:id="@+id/confirm_pass_lin"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/confirm_pass_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:contentDescription="none"
                    android:padding="9dp"
                    android:src="@drawable/password" />

                <EditText
                    android:id="@+id/confirm_pass_edit"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:hint="Confirm Password"
                    android:inputType="textPassword"
                    android:paddingLeft="13dp"
                    android:singleLine="true"
                    android:paddingEnd="@dimen/Error_end_padding"
                    android:textColor="#3C3C3C"
                    android:textColorHint="@color/lightgray02" />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="Pick Your Account URL/DNS Title"
            android:textColor="@android:color/white"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_weight="0.30"
            android:text="Your Login URL"
            android:textColor="#fff"
            android:textSize="13sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:weightSum="5" >

            <TextView
                android:id="@+id/tv2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.6"
                android:gravity="center_vertical"
                android:text="http://"
                android:textColor="#fff"
                android:textSize="12sp" />

            <EditText
                android:id="@+id/url_et"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_weight="3"
                android:background="#fff"
                android:textColor="#3C3C3C"
                android:paddingTop="5dp" >
            </EditText>

            <TextView
                android:id="@+id/tv3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:gravity="center_vertical"
                android:text=".Invoicera.com"
                android:textColor="#ffff"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical" >

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:gravity="center_vertical"
                android:text="Add me to the Mailing List"
                android:textColor="#ffffff"
                android:textSize="12dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/terms_linear"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <CheckBox
                android:id="@+id/cb_agree_terms"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:text="@string/terms"
                android:textColor="#FFF"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/terms_text_click"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#FFF"
                android:text="terms and policy " />
        </LinearLayout>

        <Button
            android:id="@+id/btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#EC6B17"
            android:paddingBottom="9dp"
            android:paddingTop="9dp"
            android:text="@string/register"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#ffffff" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/default_view_margin"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="Already have an Account ? "
                android:textColor="@android:color/white"
                android:textSize="19sp" />

            <TextView
                android:id="@+id/sign_in_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="@string/sign_in"
                android:textColor="@android:color/white"
                android:textSize="19sp"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>

</ScrollView><!-- From: file:/home/vinove/Desktop/InvoiceraAppStudioVersion/InvoiceraNewapp1/invoiceraNewApp/src/main/res/layout/sign_up.xml -->